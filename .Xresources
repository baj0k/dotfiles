!! Transparency (0-1):
st.alpha: 0.8

!! Set a default font and font size as below:
st.font: Monospace-8:antialias=true:autohint=true;

st.termname: st-256color
st.borderpx: 0

!! Set the background, foreground and cursor colors as below:
*.background: #000000
*.foreground: #DFDFDF
*.cursorColor: #C0C0C0
*.cursorshape: 4

*.color0:  #2E3436
*.color1:  #CC0000
*.color2:  #4E9A06
*.color3:  #C4A000
*.color4:  #3465A4
*.color5:  #75507B
*.color6:  #06989A
*.color7:  #D3D7CF
*.color8:  #555753
*.color9:  #EF2929
*.color10: #8AE234
*.color11: #FCE94F
*.color12: #729FCF
*.color13: #AD7FA8
*.color14: #34E2E2
*.color15: #EEEEEC
